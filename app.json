{
  "expo": {
    "name": "FluxShorts",
    "slug": "fluxshorts",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon4.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon4.png",
        "backgroundColor": "#7c2d12"
      },
      "package": "com.tunji123.fluxshorts"
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "splash": {
      "image": "./assets/images/icon4.png",
      "resizeMode": "contain",
      "backgroundColor": "#7c2d12"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "d258cfde-4743-42da-b3b5-b6b4f86e0d85"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/d258cfde-4743-42da-b3b5-b6b4f86e0d85"
    }
  }
}
